<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="user.mapper.UserTokenMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="user.entity.UserToken">
        <id column="utid" property="utid" />
        <result column="uid" property="uid" />
        <result column="language" property="language" />
        <result column="from" property="from" />
        <result column="token_content" property="tokenContent" />
        <result column="version" property="version" />
        <result column="login_time" property="loginTime" />
        <result column="exit_time" property="exitTime" />
        <result column="status" property="status" />
    </resultMap>

</mapper>
